            }

        .custom-navbar {
            height: 40px; /* Ajusta esta altura según tus necesidades */
            line-height: 40px; /* Alinea el texto verticalmente */
            font-size: 14px; /* Ajusta el tamaño de la fuente si es necesario */
            padding: 0;
        }

            .custom-navbar .navbar-text {
                padding-top: 0;
                padding-bottom: 0;
                font-size: 11px;
                color: white;
            }

        /* Overlay para cubrir toda la pantalla */
        #overlay {
            position: fixed;
            z-index: 9999;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(248, 248, 248, 0.9); /* Color con opacidad */
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center; /* Centra el texto debajo de la imagen */

        }

        /* Estilo para la imagen de carga */
        #imgWaitIcon {
            display: block;
            width: 80%; /* Ajusta automáticamente el ancho */
            height: auto; /* Ajusta automáticamente la altura */
        }

        /* Estilo para el texto */
        #loadingText {
            margin-top: 10px; /* Espacio entre la imagen y el texto */
            font-size: 16px; /* Tamaño de fuente */
            color: #000; /* Color del texto */
        }

        /*$(function(){
