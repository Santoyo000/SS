                        {
                            if (Session["alumno"] != null)
                            {
                                Response.Write("Bienvenido, " + Session["alumno"].ToString() + ": " + Session["plan"].ToString());

                                if ((int)Convert.ToInt32(Session["tipoUsuario"].ToString().Split('|')[1]) > 1 && !string.IsNullOrEmpty(Session["plan"].ToString()))
                                {
                                    lbtCambiarCarrera.Visible = true;
                                }
                            }
                            if (Session["empleado"] != null)
                            {
                                Response.Write("Bienvenido, " + Session["empleado"].ToString());
                            }
                        }
                    %>
                    <span style="margin-left: 3px;">
                        <asp:LinkButton ID="lbtCambiarCarrera" runat="server" Visible="false" OnClick="lbtCambiarCarrera_Click" Style="font-size: 11px !important;">Cambiar</asp:LinkButton>
